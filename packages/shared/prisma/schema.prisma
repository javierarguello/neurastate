generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

/// Maps to table neurastate.property_point_view
model PropertyPointView {
  x                    Decimal?  @db.Decimal
  y                    Decimal?  @db.Decimal
  objectId             Int       @id @map("objectid")
  folio                String?   @db.Text
  ttrrss               Decimal?  @db.Decimal
  xCoord               Float?    @db.DoublePrecision @map("x_coord")
  yCoord               Float?    @db.DoublePrecision @map("y_coord")
  trueSiteAddr         String?   @db.Text @map("true_site_addr")
  trueSiteUnit         String?   @db.Text @map("true_site_unit")
  trueSiteCity         String?   @db.Text @map("true_site_city")
  trueSiteZipCode      String?   @db.Text @map("true_site_zip_code")
  trueMailingAddr1     String?   @db.Text @map("true_mailing_addr1")
  trueMailingAddr2     String?   @db.Text @map("true_mailing_addr2")
  trueMailingAddr3     String?   @db.Text @map("true_mailing_addr3")
  trueMailingCity      String?   @db.Text @map("true_mailing_city")
  trueMailingState     String?   @db.Text @map("true_mailing_state")
  trueMailingZipCode   String?   @db.Text @map("true_mailing_zip_code")
  trueMailingCountry   String?   @db.Text @map("true_mailing_country")
  trueOwner1           String?   @db.Text @map("true_owner1")
  trueOwner2           String?   @db.Text @map("true_owner2")
  trueOwner3           String?   @db.Text @map("true_owner3")
  condoFlag            String?   @db.Text @map("condo_flag")
  parentFolio          String?   @db.Text @map("parent_folio")
  dorCodeCur           Decimal?  @db.Decimal @map("dor_code_cur")
  dorDesc              String?   @db.Text @map("dor_desc")
  subdivision          String?   @db.Text
  bedroomCount         Decimal?  @db.Decimal @map("bedroom_count")
  bathroomCount        Decimal?  @db.Decimal @map("bathroom_count")
  halfBathroomCount    Decimal?  @db.Decimal @map("half_bathroom_count")
  floorCount           Decimal?  @db.Decimal @map("floor_count")
  unitCount            Decimal?  @db.Decimal @map("unit_count")
  buildingActualArea   Decimal?  @db.Decimal @map("building_actual_area")
  buildingHeatedArea   Decimal?  @db.Decimal @map("building_heated_area")
  lotSize              Decimal?  @db.Decimal @map("lot_size")
  yearBuilt            Int?      @map("year_built")
  assessmentYearCur    Int?      @map("assessment_year_cur")
  assessedValCur       Decimal?  @db.Decimal @map("assessed_val_cur")
  dos1                 DateTime? @db.Date @map("dos_1")
  price1               Decimal?  @db.Decimal @map("price_1")
  legal                String?   @db.Text
  pid                  BigInt?   @db.BigInt
  dateOfSaleUtc        DateTime? @db.Timestamptz(6) @map("dateofsale_utc")
  createdAt            DateTime  @default(now()) @db.Timestamptz(6) @map("created_at")
  updatedAt            DateTime  @default(now()) @updatedAt @db.Timestamptz(6) @map("updated_at")
  geomRaw              Unsupported("geometry")? @map("geom_raw")
  geom                 Unsupported("geometry")? @map("geom")

  @@map("property_point_view")
}

/// Maps to table neurastate.settings
model Settings {
  id                   Int      @id @default(autoincrement())
  datasetPointOfViewUrl String  @db.Text @map("dataset_point_of_view_url")
  createdAt            DateTime @default(now()) @db.Timestamptz(6) @map("created_at")
  updatedAt            DateTime @default(now()) @updatedAt @db.Timestamptz(6) @map("updated_at")

  @@map("settings")
}
